    "def create_performance_dashboard(model_results, eigenvalue_data):\n",
    "    \"\"\"Create comprehensive performance comparison dashboard.\"\"\"\n",
    "    \n",
    "    # Collect all performance metrics\n",
    "    performance_data = []\n",
    "    \n",
    "    for model_name, results in model_results.items():\n",
    "        row = {'Model': model_name}\n",
    "        \n",
    "        # Training metrics\n",
    "        row['Final Train Loss'] = results.get('final_train_loss', np.nan)\n",
    "        row['Best Val Loss'] = results.get('best_val_loss', np.nan)\n",
    "        row['Total Epochs'] = results.get('total_epochs', np.nan)\n",
    "        row['Training Time (s)'] = results.get('training_time', np.nan)\n",
    "        \n",
    "        # Test metrics\n",
    "        test_metrics = results.get('test_metrics', {})\n",
    "        for metric, value in test_metrics.items():\n",
    "            row[f'Test {metric.replace(\"_\", \" \").title()}'] = value\n",
    "        \n",
    "        # Spectral metrics\n",
    "        if model_name in eigenvalue_data:\n",
    "            eigenvals = eigenvalue_data[model_name]\n",
    "            row['Spectral Radius'] = np.max(np.abs(eigenvals))\n",
    "            row['Num Eigenvalues'] = len(eigenvals)\n",
    "            row['Stable Modes'] = np.sum(np.abs(eigenvals) < 1.0)\n",
    "            row['Complex Modes'] = np.sum(np.abs(eigenvals.imag) >= 1e-10)\n",
    "        \n",
    "        performance_data.append(row)\n",
    "    \n",
    "    performance_df = pd.DataFrame(performance_data)\n",
    "    \n",
    "    print(\"Comprehensive Performance Comparison\")\n",
    "    print(\"=\" * 60)\n",
    "    display(performance_df)\n",
    "    \n",
    "    # Create performance radar chart\n",
    "    if len(performance_data) > 1:\n",
    "        create_radar_chart(performance_df)\n",
    "    \n",
    "    return performance_df\n",
    "\n",
    "def create_radar_chart(performance_df):\n",
    "    \"\"\"Create radar chart for model comparison.\"\"\"\n",
    "    \n",
    "    # Select numeric columns for radar chart\n",
    "    numeric_cols = performance_df.select_dtypes(include=[np.number]).columns\n",
    "    \n",
    "    # Remove columns with too many NaN values\n",
    "    valid_cols = []\n",
    "    for col in numeric_cols:\n",
    "        if performance_df[col].notna().sum() >= 2:  # At least 2 valid values\n",
    "            valid_cols.append(col)\n",
    "    \n",
    "    if len(valid_cols) < 3:\n",
    "        print(\"Not enough valid metrics for radar chart\")\n",
    "        return\n",
    "    \n",
    "    # Select top metrics for radar chart\n",
    "    radar_metrics = valid_cols[:6]  # Limit to 6 metrics for readability\n",
    "    \n",
    "    # Normalize metrics (0-1 scale, higher is better)\n",
    "    normalized_data = performance_df[['Model'] + radar_metrics].copy()\n",
    "    \n",
    "    for col in radar_metrics:\n",
    "        values = normalized_data[col].dropna()\n",
    "        if len(values) > 1:\n",
    "            # For loss metrics, invert (lower is better)\n",
    "            if 'loss' in col.lower() or 'error' in col.lower():\n",
    "                normalized_data[col] = 1 - (normalized_data[col] - values.min()) / (values.max() - values.min())\n",
    "            else:\n",
    "                normalized_data[col] = (normalized_data[col] - values.min()) / (values.max() - values.min())\n",
    "    \n",
    "    # Create radar chart\n",
    "    fig, ax = plt.subplots(figsize=(10, 8), subplot_kw=dict(projection='polar'))\n",
    "    \n",
    "    angles = np.linspace(0, 2 * np.pi, len(radar_metrics), endpoint=False).tolist()\n",
    "    angles += angles[:1]  # Complete the circle\n",
    "    \n",
    "    colors = plt.cm.Set1(np.linspace(0, 1, len(normalized_data)))\n",
    "    \n",
    "    for i, (_, row) in enumerate(normalized_data.iterrows()):\n",
    "        values = [row[col] if not pd.isna(row[col]) else 0 for col in radar_metrics]\n",
    "        values += values[:1]  # Complete the circle\n",
    "        \n",
    "        ax.plot(angles, values, 'o-', linewidth=2, label=row['Model'], color=colors[i])\n",
    "        ax.fill(angles, values, alpha=0.25, color=colors[i])\n",
    "    \n",
    "    ax.set_xticks(angles[:-1])\n",
    "    ax.set_xticklabels([col.replace('_', '\\n') for col in radar_metrics])\n",
    "    ax.set_ylim(0, 1)\n",
    "    ax.set_title('Model Performance Comparison\\n(Normalized Metrics)', size=14, pad=20)\n",
    "    ax.legend(loc='upper right', bbox_to_anchor=(1.3, 1.0))\n",
    "    ax.grid(True)\n",
    "    \n",
    "    plt.tight_layout()\n",
    "    plt.show()\n",
    "\n",
    "# Create performance dashboard\n",
    "performance_df = create_performance_dashboard(model_results, eigenvalue_data)"
   ]
