    "def load_model_results(results_dir='../../results/all_models_training'):\n",
    "    \"\"\"Load training results for all models.\"\"\"\n",
    "    \n",
    "    results_path = Path(results_dir)\n",
    "    model_results = {}\n",
    "    \n",
    "    # Expected model directories\n",
    "    model_dirs = {\n",
    "        'MLP': 'mlp_results',\n",
    "        'DeepONet': 'deeponet_results',\n",
    "        'LSTM': 'lstm_results'\n",
    "    }\n",
    "    \n",
    "    for model_name, dir_name in model_dirs.items():\n",
    "        model_dir = results_path / dir_name\n",
    "        \n",
    "        if not model_dir.exists():\n",
    "            print(f\"Warning: {model_name} results not found at {model_dir}\")\n",
    "            continue\n",
    "        \n",
    "        # Load training results\n",
    "        result_files = [\n",
    "            f'{model_name.lower()}_results.json',\n",
    "            'training_results.json',\n",
    "            'results.json'\n",
    "        ]\n",
    "        \n",
    "        results_loaded = False\n",
    "        for result_file in result_files:\n",
    "            result_path = model_dir / result_file\n",
    "            if result_path.exists():\n",
    "                with open(result_path, 'r') as f:\n",
    "                    model_results[model_name] = json.load(f)\n",
    "                results_loaded = True\n",
    "                break\n",
    "        \n",
    "        if not results_loaded:\n",
    "            print(f\"Warning: No results file found for {model_name}\")\n",
    "            continue\n",
    "        \n",
    "        # Load operator matrix if available\n",
    "        operator_path = model_dir / 'operator_matrix.npy'\n",
    "        if operator_path.exists():\n",
    "            model_results[model_name]['operator_matrix'] = np.load(operator_path)\n",
    "        \n",
    "        print(f\"Loaded {model_name} results\")\n",
    "    \n",
    "    return model_results\n",
    "\n",
    "# Load results\n",
    "model_results = load_model_results()\n",
    "print(f\"\\nLoaded results for {len(model_results)} models: {list(model_results.keys())}\")"
   ]
